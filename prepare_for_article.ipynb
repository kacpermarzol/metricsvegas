   "source": [
    "from skimage import io\n",
    "from skimage import transform as tf\n",
    "\n",
    "image = io.imread(\"article/teaser/m1.png\")\n",
    "\n",
    "afine_tf = tf.AffineTransform(shear=0.5)\n",
    "\n",
    "modified = tf.warp(image, inverse_map=afine_tf, mode='symmetric')\n",
    "\n",
    "io.imshow(modified)\n",
    "io.show()\n"
   ],
